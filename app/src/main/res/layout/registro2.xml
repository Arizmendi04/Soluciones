<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/contenedor"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<RelativeLayout
		android:id="@+id/paginaRegistro"
		android:layout_marginTop="0dp"
		android:layout_marginStart="0dp"
		android:layout_width="match_parent"
		android:layout_height="wrap_content">

		<View
			android:id="@+id/frame"
			android:layout_width="match_parent"
			android:layout_height="823dp"
			android:layout_marginStart="0dp"
			android:layout_marginTop="0dp"
			android:background="@drawable/_bg__registro_ek2_shape"
			android:backgroundTint="#FFFFFF" />

		<View
			android:id="@+id/contenedorinfo"
			android:layout_width="352dp"
			android:layout_height="554dp"
			android:layout_marginStart="30dp"
			android:layout_marginTop="245dp"
			android:background="@drawable/contenedorinfo_shape"
			android:backgroundTint="#EFEFEF" />

		<ImageView
			android:id="@+id/fondologin"
			android:src="@drawable/fondologin"
			android:layout_marginStart="-7dp"
			android:layout_marginTop="-100dp"
			android:layout_width="match_parent"
			android:layout_height="300dp"
			android:contentDescription="@string/descripcionFondoLogin" />

		<ImageView
			android:id="@+id/flechaatras"
			android:src="@drawable/flecharegreso"
			android:layout_marginStart="19dp"
			android:layout_marginTop="17dp"
			android:layout_width="39dp"
			android:layout_height="46dp"
			android:contentDescription="@string/flechaRegresar" />

		<ImageView
			android:id="@+id/logotipsinfondo"
			android:src="@drawable/logotipsinfondo"
			android:layout_marginStart="119dp"
			android:layout_marginTop="193dp"
			android:layout_width="157dp"
			android:layout_height="98dp"
			android:contentDescription="@string/logoSinFondo" />

		<ImageView
			android:id="@+id/iconosiguiente"
			android:layout_width="26dp"
			android:layout_height="27dp"
			android:layout_marginStart="316dp"
			android:layout_marginTop="733dp"
			android:src="@drawable/iconosiguiente"
			android:contentDescription="@string/iconoSiguiente" />

		<ImageView
			android:id="@+id/iconoatras"
			android:src="@drawable/iconoatras"
			android:layout_marginStart="66dp"
			android:layout_marginTop="733dp"
			android:layout_width="26dp"
			android:layout_height="27dp"
			android:contentDescription="@string/iconoAtras" />

		<TextView
			android:id="@+id/txtCalle"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="80dp"
			android:layout_marginTop="300dp"
			android:text="@string/txtCalle"
			android:textColor="#000000"
			android:textSize="16sp"
			android:textStyle="bold" />

		<EditText
			android:id="@+id/entradaCalle"
			android:layout_width="wrap_content"
			android:layout_height="48dp"
			android:layout_marginStart="80dp"
			android:layout_marginTop="325dp"
			android:ems="13"
			android:inputType="text"
			android:hint="@string/entradaCalle"
			android:textColor="#928F8F" />

		<TextView
			android:id="@+id/txtColonia"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="80dp"
			android:layout_marginTop="380dp"
			android:text="@string/txtColonia"
			android:textColor="#000000"
			android:textSize="16sp"
			android:textStyle="bold" />

		<EditText
			android:id="@+id/entradaColonia"
			android:layout_width="wrap_content"
			android:layout_height="48dp"
			android:layout_marginStart="80dp"
			android:layout_marginTop="400dp"
			android:ems="13"
			android:inputType="text"
			android:hint="@string/entradaColonia"
			android:textColor="#928F8F" />

		<TextView
			android:id="@+id/txtLocalidad"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="80dp"
			android:layout_marginTop="450dp"
			android:text="@string/txtLocalidad"
			android:textColor="#000000"
			android:textSize="16sp"
			android:textStyle="bold" />

		<EditText
			android:id="@+id/entradaLocalidad"
			android:layout_width="wrap_content"
			android:layout_height="48dp"
			android:layout_marginStart="80dp"
			android:layout_marginTop="470dp"
			android:ems="13"
			android:inputType="text"
			android:hint="@string/entradaLocalidad"
			android:textColor="#928F8F" />

		<TextView
			android:id="@+id/txtNoInterior"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="80dp"
			android:layout_marginTop="520dp"
			android:text="@string/txtNoInterior"
			android:textColor="#000000"
			android:textSize="16sp"
			android:textStyle="bold" />

		<EditText
			android:id="@+id/entradaNoInterior"
			android:layout_width="70dp"
			android:layout_height="48dp"
			android:layout_marginStart="80dp"
			android:layout_marginTop="535dp"
			android:ems="13"
			android:inputType="text"
			android:hint="@string/mensajeNumero"
			android:textColor="#928F8F" />

		<TextView
			android:id="@+id/txtNoExterior"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="180dp"
			android:layout_marginTop="520dp"
			android:text="@string/txtNoExterior"
			android:textColor="#000000"
			android:textSize="16sp"
			android:textStyle="bold" />

		<EditText
			android:id="@+id/entradaNoExterior"
			android:layout_width="70dp"
			android:layout_height="48dp"
			android:layout_marginStart="180dp"
			android:layout_marginTop="535dp"
			android:ems="13"
			android:inputType="text"
			android:hint="@string/mensajeNumero"
			android:textColor="#928F8F" />

		<TextView
			android:id="@+id/txtCP"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="290dp"
			android:layout_marginTop="520dp"
			android:text="@string/txtCP"
			android:textColor="#000000"
			android:textSize="16sp"
			android:textStyle="bold" />

		<EditText
			android:id="@+id/entradaCP"
			android:layout_width="70dp"
			android:layout_height="48dp"
			android:layout_marginStart="280dp"
			android:layout_marginTop="535dp"
			android:ems="13"
			android:inputType="text"
			android:hint="@string/mensajeNumero"
			android:textColor="#928F8F" />

		<TextView
			android:id="@+id/txtMunicipio"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="80dp"
			android:layout_marginTop="590dp"
			android:text="@string/txtMunicipio"
			android:textColor="#000000"
			android:textSize="16sp"
			android:textStyle="bold" />

		<EditText
			android:id="@+id/entradaMunicipio"
			android:layout_width="wrap_content"
			android:layout_height="48dp"
			android:layout_marginStart="80dp"
			android:layout_marginTop="605dp"
			android:ems="13"
			android:inputType="text"
			android:hint="@string/entradaMunicipio"
			android:textColor="#928F8F" />

		<Spinner
			android:id="@+id/comboboxEstado"
			android:layout_width="180dp"
			android:layout_height="48dp"
			android:layout_marginStart="80dp"
			android:layout_marginTop="670dp"
			android:entries="@array/spinner_items"
			android:background="@drawable/spinner_background"
			android:popupBackground="#1b9c0c"
			android:theme="@style/SpinnerStyle" />

		<ImageView
			android:id="@+id/flechaComboBox"
			android:src="@drawable/comboboxt"
			android:layout_marginStart="220dp"
			android:layout_marginTop="685dp"
			android:layout_width="15dp"
			android:layout_height="15dp"
			android:contentDescription="@string/iconoComboBox" />

	</RelativeLayout>

</RelativeLayout>

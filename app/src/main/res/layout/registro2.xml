<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/contenedor"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<androidx.constraintlayout.widget.ConstraintLayout
		android:id="@+id/paginaRegistro"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginStart="0dp"
		android:layout_marginTop="0dp">

		<View
			android:id="@+id/frame"
			android:layout_width="match_parent"
			android:layout_height="823dp"
			android:background="@drawable/_bg__registro_ek2_shape"
			android:backgroundTint="#FFFFFF"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<View
			android:id="@+id/contenedorinfo"
			android:layout_width="352dp"
			android:layout_height="554dp"
			android:background="@drawable/contenedorinfo_shape"
			android:backgroundTint="#EFEFEF"
			app:layout_constraintBottom_toBottomOf="@+id/frame"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintHorizontal_bias="0.491"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="@+id/frame"
			app:layout_constraintVertical_bias="0.903" />

		<ImageView
			android:id="@+id/fondologin"
			android:layout_width="match_parent"
			android:layout_height="300dp"
			android:layout_marginStart="-7dp"
			android:layout_marginTop="-100dp"
			android:contentDescription="@string/descripcionFondoLogin"
			android:src="@drawable/fondologin"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<ImageView
			android:id="@+id/logotipsinfondo"
			android:layout_width="157dp"
			android:layout_height="98dp"
			android:contentDescription="@string/logoSinFondo"
			android:src="@drawable/logotipsinfondo"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintHorizontal_bias="0.456"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintVertical_bias="0.264" />

		<ImageView
			android:id="@+id/iconoatras"
			android:layout_width="35dp"
			android:layout_height="41dp"
			android:layout_marginStart="28dp"
			android:layout_marginTop="24dp"
			android:contentDescription="@string/iconoAtras"
			android:src="@drawable/iconoatras"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<TextView
			android:id="@+id/txtCalle"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="80dp"
			android:layout_marginTop="300dp"
			android:text="@string/txtCalle"
			android:textColor="#000000"
			android:textSize="16sp"
			android:textStyle="bold"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<EditText
			android:id="@+id/entradaCalle"
			android:layout_width="wrap_content"
			android:layout_height="48dp"
			android:layout_marginStart="80dp"
			android:layout_marginTop="325dp"
			android:ems="13"
			android:hint="@string/entradaCalle"
			android:inputType="text"
			android:textColor="#928F8F"
			android:textColorHint="#757575"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<TextView
			android:id="@+id/txtColonia"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="80dp"
			android:layout_marginTop="380dp"
			android:text="@string/txtColonia"
			android:textColor="#000000"
			android:textSize="16sp"
			android:textStyle="bold"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<EditText
			android:id="@+id/entradaColonia"
			android:layout_width="wrap_content"
			android:layout_height="48dp"
			android:layout_marginStart="80dp"
			android:layout_marginTop="400dp"
			android:ems="13"
			android:hint="@string/entradaColonia"
			android:inputType="text"
			android:textColor="#928F8F"
			android:textColorHint="#757575"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<TextView
			android:id="@+id/txtLocalidad"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="80dp"
			android:layout_marginTop="450dp"
			android:text="@string/txtLocalidad"
			android:textColor="#000000"
			android:textSize="16sp"
			android:textStyle="bold"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<EditText
			android:id="@+id/entradaLocalidad"
			android:layout_width="wrap_content"
			android:layout_height="48dp"
			android:layout_marginStart="80dp"
			android:layout_marginTop="470dp"
			android:ems="13"
			android:hint="@string/entradaLocalidad"
			android:inputType="text"
			android:textColor="#928F8F"
			android:textColorHint="#757575"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<TextView
			android:id="@+id/txtCP"
			android:layout_width="31dp"
			android:layout_height="32dp"
			android:layout_marginStart="80dp"
			android:layout_marginTop="688dp"
			android:text="@string/txtCP"
			android:textColor="#000000"
			android:textSize="16sp"
			android:textStyle="bold"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<EditText
			android:id="@+id/entradaCP"
			android:layout_width="70dp"
			android:layout_height="48dp"
			android:layout_marginStart="124dp"
			android:layout_marginTop="676dp"
			android:ems="13"
			android:hint="@string/mensajeNumero"
			android:inputType="number"
			android:textColor="#928F8F"
			android:textColorHint="#757575"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<TextView
			android:id="@+id/txtMunicipio"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="80dp"
			android:layout_marginTop="528dp"
			android:text="@string/txtMunicipio"
			android:textColor="#000000"
			android:textSize="16sp"
			android:textStyle="bold"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<EditText
			android:id="@+id/entradaMunicipio"
			android:layout_width="wrap_content"
			android:layout_height="48dp"
			android:layout_marginStart="80dp"
			android:layout_marginTop="552dp"
			android:ems="13"
			android:hint="@string/entradaMunicipio"
			android:inputType="text"
			android:textColor="#928F8F"
			android:textColorHint="#757575"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<Spinner
			android:id="@+id/comboboxEstado"
			android:layout_width="180dp"
			android:layout_height="48dp"
			android:layout_marginStart="80dp"
			android:layout_marginTop="612dp"
			android:background="@drawable/spinner_background"
			android:entries="@array/spinner_items"
			android:popupBackground="#1b9c0c"
			android:theme="@style/SpinnerStyle"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<ImageView
			android:id="@+id/flechaComboBox"
			android:layout_width="17dp"
			android:layout_height="14dp"
			android:layout_marginStart="228dp"
			android:layout_marginTop="632dp"
			android:contentDescription="@string/iconoComboBox"
			android:src="@drawable/comboboxt"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<Button
			android:id="@+id/btnSubir"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="@drawable/bg_boton_redondo"
			android:text="Registrarme"
			android:textColor="@color/white"
			app:layout_constraintBottom_toBottomOf="@+id/frame"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintHorizontal_bias="0.541"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="@+id/frame"
			app:layout_constraintVertical_bias="0.948" />

	</androidx.constraintlayout.widget.ConstraintLayout>

</RelativeLayout>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/page_registro_ek1"
	android:layout_marginTop="0dp"
	android:layout_marginStart="0dp"
	android:layout_width="match_parent"
	android:layout_height="match_parent" >

	<View
		android:id="@+id/_bg__registro_ek2"
		android:layout_marginStart="0dp"
		android:layout_marginTop="0dp"
		android:layout_width="411dp"
		android:layout_height="823dp"
		android:background="#FFFFFF" />

	<View
		android:id="@+id/contenedorinfo"
		android:layout_width="333dp"
		android:layout_height="429dp"
		android:layout_marginStart="44dp"
		android:layout_marginTop="337dp"
		android:background="@drawable/contenedorinfo_shape"
		android:backgroundTint="#EFEFEF"
		android:contentDescription="@string/descripcionContenedorInfo" />

	<ImageView
		android:id="@+id/fondologin"
		android:layout_marginStart="-3dp"
		android:layout_marginTop="-55dp"
		android:layout_width="416dp"
		android:layout_height="321dp"
		android:src="@drawable/fondologin"
		android:contentDescription="@string/descripcionFondoLogin" />

	<ImageView
		android:id="@+id/flechaatras"
		android:layout_marginStart="19dp"
		android:layout_marginTop="17dp"
		android:layout_width="39dp"
		android:layout_height="46dp"
		android:src="@drawable/flecharegreso"
		android:contentDescription="@string/descripcionFlechaRegreso" />

	<ImageView
		android:id="@+id/logotipsinfondo_1"
		android:layout_marginStart="123dp"
		android:layout_marginTop="288dp"
		android:layout_width="157dp"
		android:layout_height="98dp"
		android:src="@drawable/logotipsinfondo_1"
		android:contentDescription="@string/descripcionLogoSinFondo" />

	<ImageView
		android:id="@+id/iconosiguiente"
		android:layout_marginStart="321dp"
		android:layout_marginTop="690dp"
		android:layout_width="26dp"
		android:layout_height="27dp"
		android:src="@drawable/iconosiguiente"
		android:contentDescription="@string/descripcionIconoSiguiente" />

	<ImageView
		android:id="@+id/iconoatras"
		android:layout_marginStart="70dp"
		android:layout_marginTop="690dp"
		android:layout_width="26dp"
		android:layout_height="27dp"
		android:src="@drawable/iconoatras"
		android:contentDescription="@string/descripcionIconoAtras" />

	<ImageView
		android:id="@+id/iconosubirdocumento"
		android:layout_marginStart="155dp"
		android:layout_marginTop="485dp"
		android:layout_width="120dp"
		android:layout_height="80dp"
		android:src="@drawable/iconosubirdocumento"
		android:contentDescription="@string/descripcionIconoSubirDocumento"/>

	<TextView
		android:id="@+id/textoDoc"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="155dp"
		android:layout_marginTop="570dp"
		android:text="@string/txtDoc"
		android:textColor="#000000"
		android:textSize="20sp" />

	<TextView
		android:id="@+id/textoFlechaSig"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="300dp"
		android:layout_marginTop="725dp"
		android:text="@string/flechaSig"
		android:textColor="#000000"
		android:textSize="16sp" />

	<TextView
		android:id="@+id/textoFlechaAtras"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="65dp"
		android:layout_marginTop="725dp"
		android:text="@string/flechaAtras"
		android:textColor="#000000"
		android:textSize="16sp"/>

	<TextView
		android:id="@+id/mensajeSubirDoc"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="90dp"
		android:layout_marginTop="390dp"
		android:text="@string/msjAdjuntarDoc"
		android:textColor="#000000"
		android:textSize="20sp"
		android:textStyle="bold"/>

</RelativeLayout>

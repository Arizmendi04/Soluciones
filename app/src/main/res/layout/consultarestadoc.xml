<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/page_consultarestadoc_ek1"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<View
		android:id="@+id/_bg__consultarestadoc_ek2"
		android:background="@drawable/shape_consultar_estado"
		android:layout_width="match_parent"
		android:layout_height="match_parent" />

	<ImageView
		android:id="@+id/contenedorimagen"
		android:src="@drawable/contenedorimagen"
		android:contentDescription="contenedorimagen"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:scaleType="centerCrop"
		android:layout_alignParentTop="true" />

	<ImageView
		android:id="@+id/flecharegreso_1"
		android:src="@drawable/flecharegreso_1"
		android:contentDescription="flecharegreso_1"
		android:layout_width="30dp"
		android:layout_height="35dp"
		android:layout_marginStart="25dp"
		android:layout_marginTop="16dp"
		android:layout_alignParentStart="true"
		android:layout_alignParentTop="true" />

	<ImageView
		android:id="@+id/iconodescargar_1"
		android:src="@drawable/iconodescargar_1"
		android:contentDescription="iconodescargar_1"
		android:layout_width="20dp"
		android:layout_height="21dp"
		android:layout_marginStart="53dp"
		android:layout_marginTop="16dp"
		android:layout_alignParentEnd="true"
		android:layout_alignParentBottom="true" />

	<ImageView
		android:id="@+id/letraslogosinfondo_1"
		android:src="@drawable/letraslogosinfondo_1"
		android:contentDescription="letraslogosinfondo_1"
		android:layout_width="136dp"
		android:layout_height="71dp"
		android:layout_marginStart="137dp"
		android:layout_marginTop="105dp" />

	<ImageView
		android:id="@+id/logotipsinfondo_1"
		android:src="@drawable/logotipsinfondo_1"
		android:contentDescription="logotipsinfondo_1"
		android:layout_width="90dp"
		android:layout_height="84dp"
		android:layout_marginStart="158dp"
		android:layout_marginTop="21dp" />

	<TextView
		android:id="@+id/txtMsjB"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="70dp"
		android:layout_marginTop="220dp"
		android:text="@string/msjEstadoCuenta"
		android:textSize="20sp"
		android:textStyle="bold"/>

	<TextView
		android:id="@+id/txtDiasCredito"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="70dp"
		android:layout_marginTop="290dp"
		android:text="@string/txtDiasCredito"
		android:theme="@style/estiloEstadoCuenta"/>

	<TextView
		android:id="@+id/txtMontoCredito"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="70dp"
		android:layout_marginTop="330dp"
		android:text="@string/txtMontoCredito"
		android:theme="@style/estiloEstadoCuenta"/>

	<TextView
		android:id="@+id/txtPorPagar"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="70dp"
		android:layout_marginTop="370dp"
		android:text="@string/txtPorPagar"
		android:theme="@style/estiloEstadoCuenta"/>

	<TextView
		android:id="@+id/txtVencido"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="70dp"
		android:layout_marginTop="410dp"
		android:text="@string/txtVencido"
		android:theme="@style/estiloEstadoCuenta"/>

	<TextView
		android:id="@+id/txtCorriente"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="70dp"
		android:layout_marginTop="450dp"
		android:text="@string/txtCorriente"
		android:theme="@style/estiloEstadoCuenta"/>

	<TextView
		android:id="@+id/txtVencidoPor"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="70dp"
		android:layout_marginTop="490dp"
		android:text="@string/txtVencidoPor"
		android:theme="@style/estiloEstadoCuenta"/>

	<TextView
		android:id="@+id/btnDescargarEC"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="100dp"
		android:layout_marginTop="540dp"
		android:text="@string/descargarEC"
		android:textStyle="bold"/>

	<RelativeLayout
		android:id="@+id/opciones"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_alignParentBottom="true">

		<View
			android:id="@+id/iconosopciones"
			android:background="@drawable/iconosopciones_shape"
			android:layout_width="match_parent"
			android:layout_height="63dp" />


		<ImageView
			android:id="@+id/iconomenu_1"
			android:src="@drawable/iconomenu_1"
			android:contentDescription="iconomenu_1"
			android:layout_width="36dp"
			android:layout_height="32dp"
			android:layout_marginStart="247dp"
			android:layout_marginTop="8dp" />

		<ImageView
			android:id="@+id/iconocompra_1"
			android:src="@drawable/iconocompra_1"
			android:contentDescription="iconocompra_1"
			android:layout_width="38dp"
			android:layout_height="43dp"
			android:layout_marginStart="143dp"
			android:layout_marginTop="2dp" />

		<ImageView
			android:id="@+id/iconocerrarsesion__1"
			android:src="@drawable/iconocerrarsesion__1"
			android:contentDescription="iconocerrarsesion__1"
			android:layout_width="39dp"
			android:layout_height="35dp"
			android:layout_marginStart="332dp"
			android:layout_marginTop="8dp" />

		<ImageView
			android:id="@+id/iconolistaproductos_1"
			android:src="@drawable/iconolistaproductos_1"
			android:contentDescription="iconolistaproductos_1"
			android:layout_width="31dp"
			android:layout_height="35dp"
			android:layout_marginStart="45dp"
			android:layout_marginTop="5dp" />

	</RelativeLayout>

	<ImageView
		android:id="@+id/imageButton"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="70dp"
		android:layout_marginTop="540dp"
		app:srcCompat="@drawable/iconodescargar_1" />

	<TextView
		android:id="@+id/txtVerProductos"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="20dp"
		android:layout_marginTop="800dp"
		android:text="@string/txtVerProductos"
		android:textSize="12sp"
		android:textColor="#FFFFFF"/>

	<TextView
		android:id="@+id/txtCarrito"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="120dp"
		android:layout_marginTop="800dp"
		android:text="@string/txtCarrito"
		android:textSize="12sp"
		android:textColor="#FFFFFF"/>

	<TextView
		android:id="@+id/txtMenu"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="230dp"
		android:layout_marginTop="800dp"
		android:text="@string/txtMenuPrincipal"
		android:textSize="12sp"
		android:textColor="#FFFFFF"/>

	<TextView
		android:id="@+id/txtCerrarSesion"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="320dp"
		android:layout_marginTop="800dp"
		android:text="@string/txtCerrarSesion"
		android:textSize="12sp"
		android:textColor="#FFFFFF"/>

	<EditText
		android:id="@+id/txt_contenido_pdf"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="100dp"
		android:layout_marginTop="600dp"
		android:ems="10"
		android:inputType="text"
		android:text="Name"
		android:textSize="12sp"/>

</RelativeLayout>
